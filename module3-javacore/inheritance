class Animal
{
    public void makeSound()
    {
        System.out.println("Animal makes sound");
    }
}
class Dog extends Animal
{
    public void makeSound()
    {
        System.out.println("Bark");
    }
}
public class Inheritance
{
    public static void main(String[] args)
    {
        Animal an=new Animal();
        an.makeSound();
        Dog dg=new Dog();
        dg.makeSound();
    }
}
